;   param=value ; assign values to parameters

config_version=5

[application]

config/name="MonsterManagement"
run/main_scene="res://Home Screen/MainMenu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/image="res://Assets/Images/GameLogo.png"
config/icon="res://icon.svg"
config/windows_native_icon="res://Assets/Images/NexusIcon.ico"
boot_splash/minimum_display_time=3000

[autoload]

GlobalVariables="*res://global_variables.gd"
Lore="*res://Level Select/experiment_details.gd"

[debug]

gdscript/warnings/unassigned_variable=2
gdscript/warnings/unassigned_variable_op_assign=2
gdscript/warnings/integer_division=0

[display]

window/stretch/mode="canvas_items"

[gui]

theme/custom_font="res://Assets/Fonts/modeseven.TTF"
